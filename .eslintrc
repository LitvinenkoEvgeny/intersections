{
  "env": {
      "browser": true,
      "es6": true
  },
  "extends": ["google", "eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
     "ecmaVersion": 6,
      "ecmaFeatures": {
          "experimentalObjectRestSpread": true,
          "jsx": true
      },
      "sourceType": "module"
  },
  "plugins": [
      'react',
      'babel'
  ],
  "rules": {
      "react/prop-types": 2,
  }
}
